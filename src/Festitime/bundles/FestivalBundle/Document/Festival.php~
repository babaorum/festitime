<?php

namespace Festitime\bundles\FestivalBundle\Document;



/**
 * Festitime\bundles\FestivalBundle\Document\Festival
 */
class Festival
{
    public function toArray()
    {
        return array(
            'id' => $this->id,
            'name' => $this->name,
            'description' => $this->description,
            'type' => $this->type
        );
    }

}
