<?php

namespace Festitime\bundles\UserBundle\Document;

/**
 * Festitime\bundles\UserBundle\Document\User
 */
class User
{
    public function toArray()
    {
        return array(
            'id' => $this->id,
            'name' => $this->name,
            'description' => $this->description
        );
    }

}
